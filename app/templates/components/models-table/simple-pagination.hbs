{{! Custom pagination template for ember-models-table addon }}
<div class="btn-toolbar pull-right" role="toolbar">
    <div class="btn-group" role="group">
        <a href="#" {{action "gotoFirst"}} class="{{if gotoBackEnabled "enabled" "disabled"}} btn btn-success">
            {{fa-icon 'angle-double-left'}}
        </a>
        <a href="#" {{action "gotoPrev"}} class="{{if gotoBackEnabled "enabled" "disabled"}} btn btn-success">
            {{fa-icon 'angle-left'}}
        </a>
        <a href="#" {{action "gotoNext"}} class="{{if gotoForwardEnabled "enabled" "disabled"}} btn btn-success">
            {{fa-icon 'angle-right'}}
        </a>
        <a href="#" {{action "gotoLast"}} class="{{if gotoForwardEnabled "enabled" "disabled"}} btn btn-success">
            {{fa-icon 'angle-double-right'}}
        </a>
    </div>
</div>